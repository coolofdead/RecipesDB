ients":["5e4a42ab179521032f127c34","5c8654f9e3f33953b40b23b3","5fbe2dd103cb4c62ac01428c","5c8bcd9be3f3397358648c93"],"utensils":["57e2674fb7e8697d4b305369"]},{"index":4,"instructionsMarkdown":"Placez le poulet sur les nouilles et versez la sauce par-dessus. Saupoudrez avec le vert de l'oignon nouveau et le paprika. M\u00e9langez bien le tout avant de d\u00e9guster. Servez le concombre aigre-doux dans des petits bols \u00e0 c\u00f4t\u00e9.","instructionsHTML":"<p>Placez le poulet sur les nouilles et versez la sauce par-dessus. Saupoudrez avec le vert de l'oignon nouveau et le paprika. M\u00e9langez bien le tout avant de d\u00e9guster. Servez le concombre aigre-doux dans des petits bols \u00e0 c\u00f4t\u00e9.<\/p>","instructions":"Placez le poulet sur les nouilles et versez la sauce par-dessus. Saupoudrez avec le vert de l'oignon nouveau et le paprika. M\u00e9langez bien le tout avant de d\u00e9guster. Servez le concombre aigre-doux dans des petits bols \u00e0 c\u00f4t\u00e9.","timers":[],"images":[{"link":"https:\/\/d3hvwccx09j84u.cloudfront.net\/0,0\/625fc47827341475ef6225b3\/step-20b17037.jpg","path":"\/625fc47827341475ef6225b3\/step-20b17037.jpg","caption":"Servir"}],"videos":[],"ingredients":["5c8bcda7e3f339735a76c462"],"utensils":["57e2674fb7e8697d4b305369"]}],"wines":[],"marketplaceItems":[],"author":null,"label":null,"yieldType":"0","averageRating":3.6,"ratingsCount":351,"favoritesCount":310,"active":true,"highlighted":false,"isDinnerToLunch":false,"isExcludedFromIndex":false,"isAddon":false,"isPremium":false,"websiteUrl":"https:\/\/www.hellofresh.fr\/recipes\/poulet-epice-au-lait-de-coco-facon-bicol-625fc47827341475ef6225b3","clonedFrom":null,"canonical":null,"canonicalLink":null,"promotion":null,"uniqueRecipeCode":null,"uuid":null}